# Increase volume
"volumeControl.sh up"
   XF86AudioRaiseVolume

# Decrease volume
"volumeControl.sh down"
   XF86AudioLowerVolume

# Mute volume
"volumeControl.sh mute"
   XF86AudioMute

# Mute microphone
"pactl set-source-mute @DEFAULT_SOURCE@ toggle"
   XF86AudioMicMute

#BrightnessUp
"brightnessControl.sh up"
   c:233
   XF86MonBrightnessUp

#BrightnessDown
"brightnessControl.sh down"
   c:232
   XF86MonBrightnessDown
